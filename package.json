{
  "name": "struk",
  "displayName": "struk",
  "description": "Generic template for HTML, CSS & JS, React, Bootstrap and Python",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "publisher": "zirith",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "struk.htm",
        "title": "Struk Template for HTML, CSS, JS"
        
      },
      {
        "command": "struk.bootstrap",
        "title": "Struk Template for HTML, CSS (Bootstrap), JS"
        
      },
      {
        "command": "struk.react",
        "title": "Struk Template for React App"
        
      },
      {
        "command": "struk.python",
        "title": "Struk Template for Python"
        
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zirith001/struk.git"
  },
  "homepage": "https://github.com/zirith001/struk/blob/main/README.md"
}
